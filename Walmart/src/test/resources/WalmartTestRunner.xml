<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<suite name="Walmart" parallel="tests">

    <parameter name="useCloudEnv" value="false"/>
    <parameter name="cloudEnvName" value="browserstack"/>
    <parameter name="os" value="Windows"/>
    <parameter name="os_version" value="10"/>
    <parameter name="browserName" value="firefox"/>
    <parameter name="browserVersion" value="53.0"/>
    <parameter name="url" value="https://www.walmart.com"/>

    <test name = "Walmart Home">
        <classes>
            <!--<class name="departmentstab.Departments"/>-->
            <class name="findastore.Store"/>
        </classes>
    </test>
</suite>